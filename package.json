{
  "name": "plopjs-demo",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "plop": "NODE_ENV=development plop",
    "build": "rimraf dist && tsc",
    "test": "NODE_ENV=testing jest --detectOpenHandles --logHeapUsage",
    "consumer": "node -r module-alias/register dist/servers/consumer/consumer.server.js",
    "consumer:dev": "NODE_ENV=development tsc-watch --onSuccess \"yarn consumer\"",
    "consumer:staging": "NODE_ENV=staging yarn consumer",
    "consumer:production": "NODE_ENV=production yarn consumer",
    "admin": "yarn copy-email-templates && node -r module-alias/register dist/servers/admin/admin.server.js",
    "admin:dev": "NODE_ENV=development tsc-watch --onSuccess \"yarn admin\"",
    "admin:staging": "NODE_ENV=staging yarn admin",
    "admin:production": "NODE_ENV=production yarn admin"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/express": "^4.17.17",
    "@types/express-mung": "^0.5.2",
    "@types/jest": "^29.5.2",
    "@types/signale": "^1.4.4",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-jest": "27.0.1",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^29.6.1",
    "plop": "2.7.4",
    "plop-pack-json-modify": "^1.1.0",
    "supertest": "^6.1.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.0.0",
    "tsc-watch": "^4.4.0",
    "tsload": "^1.0.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@decorators/di": "^2.0.0",
    "@decorators/express": "^2.9.3",
    "@prisma/client": "^4.16.2",
    "body-parser": "^1.19.0",
    "camelcase": "6.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.9",
    "dotenv": "^10.0.0",
    "express": "4.17.1",
    "express-mung": "^0.5.1",
    "express-request-id": "1.4.1",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "nanoid": "3.1.31",
    "pascal-case": "^3.1.2",
    "prisma": "^4.16.2",
    "signale": "^1.4.0",
    "snake-case": "^3.0.4",
    "source-map-support": "^0.5.21",
    "title-case": "^3.0.3"
  },
  "_moduleAliases": {
    "@shared": "./dist/shared",
    "@servers": "./dist/servers",
    "@controllers": "./dist/controllers",
    "@dtos": "./dist/dtos",
    "@enums": "./dist/enums",
    "@exceptions": "./dist/exceptions",
    "@interfaces": "./dist/interfaces",
    "@middlewares": "./dist/middlewares",
    "@models": "./dist/models",
    "@providers": "./dist/providers",
    "@queues": "./dist/queues",
    "@responses": "./dist/responses",
    "@repositories": "./dist/repositories",
    "@selects": "./dist/selects",
    "@secrets": "./dist/secrets",
    "@services": "./dist/services",
    "@test": "./dist/test",
    "@utils": "./dist/utils"
  }
}
